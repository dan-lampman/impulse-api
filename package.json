{
  "author:": "Dan Lampman <dan.lampman@gmail.com>",
  "main": "./src/api.js",
  "name": "impulse-api",
  "description": "A efficient and powerful API with Express under the hood.",
  "license": "MIT",
  "pre-commit": [
    "test"
  ],
  "scripts": {
    "lint": "eslint --ignore-path .gitignore .",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "mocha test/unit/**/*.js",
    "test:integration": "mocha test/integration/**/*-test.js",
    "test-server": "node ./test/integration/test-server.js"
  },
  "version": "3.0.11",
  "engines": {
    "node": ">=22"
  },
  "dependencies": {
    "async": "2.6.4",
    "cors": "2.8.5",
    "express": "4.21.2",
    "express-fileupload": "1.5.1",
    "http-errors": "2.0.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "4.17.21",
    "morgan": "1.10.0",
    "path-to-regexp": "0.1.12"
  },
  "devDependencies": {
    "eslint": "^9.23.0",
    "eslint-plugin-import": "^2.31.0",
    "mocha": "^10.2.0",
    "pre-commit": "^1.2.2",
    "sinon": "4.4.6"
  },
  "keywords": [
    "http",
    "web-server"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dan-lampman/impulse-api.git"
  }
